{

  "description": "Adds a google bang in the duckduckgo search",
  "manifest_version": 2,
  "browser_specific_settings": {
    "gecko": {
      "id": "bangdd@yourdomain.com",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  },
  "name": "BangDD",
  "version": "0.2.2",
  "homepage_url": "https://github.com/ylogx/BangDD",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.duckduckgo.com/*"],
      "js": ["bangdd.js"]
    }
  ]

}
